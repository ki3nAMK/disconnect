{
	"name": "GeTON",
	"version": "1.0.0",
	"description": "GeTON",
	"author": "1bitlab",
	"private": true,
	"license": "1bitlab",
	"scripts": {
		"prepare": "test -d node_modules/husky && husky install || echo \"husky is not installed\"",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"docker:dev": "docker compose --env-file ./.env.dev -f docker-compose.yml up",
		"docker:dev:config": "docker compose --env-file ./.env.dev -f docker-compose.yml config",
		"docker:dev:remove": "docker compose --env-file ./.env.dev -f docker-compose.yml down",
		"start:debug": "nest start --debug --watch",
		"start:prod": "NODE_ENV=production node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"seed:user": "npx nestjs-command seed:user",
		"seed:service": "npx nestjs-command seed:service"
	},
